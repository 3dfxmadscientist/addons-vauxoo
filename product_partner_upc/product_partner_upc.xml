<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
		
		<record model="ir.ui.view" id="view_product_partner_upc_form">
			<field name="name">view.product.partner.upc.form</field>
			<field name="model">product.partner.upc</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Product Partner UPC">
					<field name="upc" select="1"/>
					<field name="partner_id" select="1"/>
					<field name="description" select="1" colspan="4"/>
					<!--<field name="active" select="1"/>-->
				</form>
			</field>
		</record>
		<record model="ir.ui.view" id="view_product_partner_upc_tree">
			<field name="name">view.product.partner.upc.tree</field>
			<field name="model">product.partner.upc</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Product Partner UPC" editable="bottom">
					<field name="upc"/>
					<field name="partner_id"/>
					<field name="description"/>
				</tree>
			</field>
		</record>
		<!--
		<record model="ir.actions.act_window" id="action_product_partner_upc">
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">product.partner.upc</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
		<menuitem name="Products/Partner UPCs" action="action_product_partner_upc" id="menu_product_partner_upc"/>
		-->
    </data>
</openerp>

