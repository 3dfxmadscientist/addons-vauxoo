  <images/>
  <story>
                <blockTable colWidths="195.0mm" style="TABLA_SIN">
                  <tr>
                    <td>
                        <para style="IZQDA2">RAZON SOCIAL: [[o.partner_id.name and  ('%s'%(o.partner_id.name.upper())) or '' ]]</para>
